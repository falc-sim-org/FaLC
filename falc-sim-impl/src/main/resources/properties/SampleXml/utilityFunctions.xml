	<!-- Columns from location_zones table which are preloaded as functions parameters at the begining of each year -->
	<params>
		<param id="res_tot"/>
		<param id="emp_tot"/>
		<param id="flat_rent"/>
		<param id="av_1"/>
		<param id="av_2"/>
		<param id="av_3"/>
		<param id="av_4"/>
		<param id="av_5"/>
		<param id="av_6"/>
		<param id="settlement_area"/>
		<param id="urban_centre"/>
		<param id="resident_landprice_n"/>
		<param id="landuse_density_n"/>
		<param id="university_degree_n"/>
		<param id="diversity_sector_n"/>
		<param id="tax_holdingcomp_n"/>
		<param id="tax_partnership_n"/>
		<param id="tax_companies_n"/>
		<param id="motorway_access"/>
		<param id="railway_access"/>
		<param id="accessibility_t_n"/>
		<param id="bus_dev_mun_n"/>
		<param id="bus_dev_cant_n"/>
	</params>

	<functions>   
		<!-- +([b_beta_13])*accessibility_t_n  -->
		<!-- +([b_beta_14])*bus_dev_mun_n -->
		<utfunction_b id="UTF_B_type1"  
			value_base="([b_beta_2])*resident_landprice_n
				+([b_beta_3])*landuse_density_n
				+([b_beta_4])*university_degree_n
				+([b_beta_7])*diversity_sector_n
				+([b_beta_10])*tax_companies_n
				+([b_beta_11])*motorway_access
				+([b_beta_12])*railway_access
				+([b_beta_13])*((av_2 + av_4 - 11929) / 10972 )
				+([b_beta_15])*bus_dev_cant_n"
			value_move=" ([b_beta_6])*_bs_glbr_q_norm + ([b_beta_1])*exp(_distance*[b_lambda])+([b_beta_5])*urban_centre"
			/>
		<utfunction_b id="UTF_B_type2"  
			value_base="([b_beta_2])*resident_landprice_n
				+([b_beta_3])*landuse_density_n
				+([b_beta_4])*university_degree_n
				+([b_beta_7])*diversity_sector_n
				+([b_beta_9])*tax_partnership_n
				+([b_beta_11])*motorway_access
				+([b_beta_12])*railway_access
				+([b_beta_13])*((av_2 + av_4 - 11929) / 10972 )
				+([b_beta_15])*bus_dev_cant_n"
			value_move=" ([b_beta_6])*_bs_glbr_q_norm + ([b_beta_1])*exp(_distance*[b_lambda])+([b_beta_5])*urban_centre"
			/>
		<utfunction_b id="UTF_B_type3"  
			value_base="([b_beta_2])*resident_landprice_n
				+([b_beta_3])*landuse_density_n
				+([b_beta_4])*university_degree_n
				+([b_beta_7])*diversity_sector_n
				+([b_beta_10])*tax_companies_n
				+([b_beta_11])*motorway_access
				+([b_beta_12])*railway_access
				+([b_beta_13])*((av_2 + av_4 - 11929) / 10972 )
				+([b_beta_15])*bus_dev_cant_n"
			value_move=" ([b_beta_6])*_bs_glbr_q_norm + ([b_beta_1])*exp(_distance*[b_lambda]) + ([b_beta_5])*urban_centre"
			/>
		
		<utfunction_h id="UTF_H"  
            value_base="([h_beta_6])*ln(av_3+av_4) + ([h_beta_7])*ln(av_1+av_2)"
            value_move=" ([h_beta_1])*(_distance ^ [h_eta_prevloc]) + ([h_beta_2])*(_distance_wp ^ [h_eta_work]) 
                                        + ([h_beta_4])*_portion_hh_sa + ([h_beta_5])*_portion_hh_ss"
            value_move_migrants=" ([h_beta_3])*(_distance ^ [h_eta_prevloc]) + ([h_beta_4])*_portion_hh_sa + ([h_beta_5])*_portion_hh_ss"
		/>
	</functions>
</config>
 